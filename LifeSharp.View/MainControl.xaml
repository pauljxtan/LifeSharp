<UserControl x:Class="LifeSharp.View.MainControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ViewModel="clr-namespace:LifeSharp.ViewModel;assembly=LifeSharp.ViewModel"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             d:DataContext="{d:DesignInstance ViewModel:Presenter}">
    <StackPanel>
        <TextBlock Text="{Binding TestText}" Margin="5"></TextBlock>
        <Button Command="{Binding EvolveCommand}" Margin="5">Evolve</Button>
    </StackPanel>
</UserControl>
