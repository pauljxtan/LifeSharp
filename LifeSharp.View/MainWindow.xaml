<Window x:Class="LifeSharp.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ViewModel="clr-namespace:LifeSharp.ViewModel;assembly=LifeSharp.ViewModel"
        xmlns:View="clr-namespace:LifeSharp.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="640" Width="640">
    <Window.DataContext>
        <ViewModel:UniverseViewModel></ViewModel:UniverseViewModel>
    </Window.DataContext>
    <StackPanel>
        <View:UniverseView></View:UniverseView>
        <View:LoadFileView></View:LoadFileView>
    </StackPanel>
</Window>
