<mui:ModernWindow x:Class="LifeSharp.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:vm="clr-namespace:LifeSharp.ViewModel"
        xmlns:v="clr-namespace:LifeSharp.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="640" Width="800"
        ContentSource="/View/MainView.xaml">

    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="LifeSharp">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="Main" Source="/View/MainView.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
</mui:ModernWindow>
